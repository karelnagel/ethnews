{
  "name": "my-app",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "start":"node --loader ts-node/esm ./src/main.ts",
    "start:old": "node dist/main.js",
    "dev": "nodemon --ext js,ts,json,env --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/main.ts",
    "prepare": "husky install",
    "build": "tsc --project tsconfig.build.json",
    "build:clean": "rm -rf tsconfig.build.tsbuildinfo && rm -rf ./dist",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles",
    "test:coverage": "npm run test -- --coverage",
    "test:ci": "npm run test -- --colors --coverage --ci",
    "lint": "eslint --ext .ts,.js .",
    "format": "prettier \"./**\" --write --ignore-unknown",
    "format:check": "prettier \"./**\" --ignore-unknown --check",
    "remotion:start": "remotion preview src/remotion/index.tsx",
    "remotion:build": "remotion render src/remotion/index.tsx HelloWorld out/video.mp4",
    "remotion:upgrade": "remotion upgrade"
  },
  "dependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@jest/globals": "^28.0.0",
    "@jest/types": "^28.0.0",
    "@remotion/bundler": "^3.0.0",
    "@remotion/cli": "^3.0.19",
    "@remotion/eslint-config": "^3.0.0",
    "@remotion/renderer": "^3.0.19",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.43",
    "@types/react": "^18.0.12",
    "@types/supertest": "^2.0.12",
    "@types/web": "^0.0.61",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "openai": "^3.0.0",
    "prettier": "^2.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remotion": "^3.0.19",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.8.1",
    "tslib": "^2.3.1",
    "twitter-api-sdk": "^1.0.9",
    "twitter-api-v2": "^1.12.2",
    "typescript": "^4.7.3"
  }
}
